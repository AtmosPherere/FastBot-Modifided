namespace fastbotx;

// widget_hash -> count
table WidgetCount
{
    widget_hash:ulong;
    count:int;
}

// activity -> {widget_hash -> count}
table ActivityWidgetMap
{
    activity:string;
    widgets:[WidgetCount];
}

// action_hash -> {activity -> {widget_hash -> count}}
table ReuseEntry
{
    action:ulong (key);
    activities:[ActivityWidgetMap];
}

// 整个模型
table WidgetReuseModel
{
    model:[ReuseEntry];
}

root_type WidgetReuseModel;